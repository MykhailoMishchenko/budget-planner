{
	"printWidth": 100,
	"eslintIntegration": true,
	"tabWidth": 4,
	"useTabs": true,
	"semi": true,
	"singleQuote": false,
	"jsxSingleQuote": false,
	"trailingComma": "es5",
	"bracketSpacing": true,
	"jsxBracketSameLine": false,
	"arrowParens": "avoid",
	"endOfLine": "auto",
	"organizeImportsSkipDestructiveCodeActions": true,
	"tailwindConfig": "./tailwind.config.js",
	"plugins": ["prettier-plugin-tailwindcss"]
}
